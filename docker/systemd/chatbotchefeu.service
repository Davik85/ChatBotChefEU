[Unit]
Description=ChatBotChef EU Telegram Bot
After=network.target

[Service]
Type=simple
EnvironmentFile=/etc/chatbotchefeu/.env
WorkingDirectory=/opt/chatbotchefeu
ExecStart=/opt/chatbotchefeu/bin/ChatBotChefEU
User=chatbot
Restart=on-failure
RestartSec=5
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target
